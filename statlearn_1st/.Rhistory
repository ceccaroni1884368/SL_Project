library(plotly)
df <- data.frame("Task" = c("Data Collect", "Models Development",
"Models test", "Updating results", "Presentation"),
"Start" = c("3/30/2020", "4/20/2020", "5/10/2020",
"5/18/2020", "5/25/2020"), # MM/GG/AAAA
"Duration" = c(60,35,6,8,10), # Days
stringsAsFactors = F)
df$Start  <- as.Date(df$Start, format = "%m/%d/%Y")
client    <- "Sample Client"
cols      <- RColorBrewer::brewer.pal(length(unique(df$Task)), name = "Set3")
df$color  <- factor(df$Task, labels = cols)
p <- plot_ly(type='scatter', mode='markers')
for(i in 1:(nrow(df) - 1)){
p <- add_trace(p,
x = c(df$Start[i], df$Start[i] + df$Duration[i]),
y = c(i, i),
mode = "lines",
line = list(color = df$color[i], width = 20),
showlegend = F,
hoverinfo = "text",
text = paste("Task: ", df$Task[i], "<br>",
"Duration: ", df$Duration[i], "days<br>")
)
}
p <- p%>%
layout(
yaxis = list(
autotick = FALSE,
ticks = "outside",
tick0 = 0,
dtick = 0.25,
ticktext = as.list(df$Task),
tickvals = as.list(1:length(df$Task)),
tickmode = "array"
))
orca(p, 'plot.pdf')
